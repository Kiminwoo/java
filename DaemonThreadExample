package Practice;

public class DaemonThreadExample {

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		while(true) 
		{
			try 
			{
				Thread.sleep(3 * 1000);
				
			}
			catch (InterruptedException e) {}
			
			if(autoSave) 
			{
				autoSave();
			}
		}

	}

}
